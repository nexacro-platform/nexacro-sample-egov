CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE IDS(TABLE_NAME VARCHAR(16) NOT NULL PRIMARY KEY,NEXT_ID DECIMAL(30) NOT NULL)
CREATE MEMORY TABLE CUST_INFO(ID VARCHAR(10),NAME VARCHAR(20),REG_NO VARCHAR(13),ADDR VARCHAR(200),TEL_NO VARCHAR(20),MOBILE VARCHAR(20),EMAIL VARCHAR(50),USE_YN VARCHAR(1),REG_USER VARCHAR(10),REG_DATE DATE)
CREATE MEMORY TABLE TBGTMS_CMCD(GTMS_CMCD_ID VARCHAR(20),GTMS_CMCD_NM VARCHAR(50),GTMS_CMCD_VAL VARCHAR(50),GTMS_CMCD_DES VARCHAR(200),GTMS_CMCD_ORD DECIMAL(4),GTMS_CMCD_PID VARCHAR(20),GTMS_USE_YN VARCHAR(1))
CREATE MEMORY TABLE TBGTMS_DEPT(DEPT_CD VARCHAR(5),DEPT_NM VARCHAR(50),REG_USER VARCHAR(10),REG_DATE DATE)
CREATE MEMORY TABLE TBGTMS_TEAM(DEPT_CD VARCHAR(5),TEAM_CD VARCHAR(20),TEAM_NM VARCHAR(50),REG_USER VARCHAR(10),REG_DATE DATE)
CREATE MEMORY TABLE TBGTMS_GRADE(GRADE_CD VARCHAR(5),GRADE_NM VARCHAR(50),REG_USER VARCHAR(10),REG_DATE DATE)
CREATE MEMORY TABLE TBGTMS_PROJECT(PROJECT_CD VARCHAR(5),PROJECT_NM VARCHAR(50),REG_USER VARCHAR(10),REG_DATE DATE)
CREATE MEMORY TABLE TBGTMS_CODE(CODE_ID VARCHAR(5) NOT NULL PRIMARY KEY,CODE_NM VARCHAR(50),USE_YN VARCHAR(2),REG_USER VARCHAR(10),REG_DATE DATE,UPD_USER VARCHAR(10),UPD_DATE DATE)
CREATE MEMORY TABLE SAMPLE(ID VARCHAR(20),SEQ INTEGER,NAME VARCHAR(20),DESCRIPTION VARCHAR(200),USE_YN VARCHAR(1),REG_USER VARCHAR(20))
CREATE MEMORY TABLE TBGTMS_USER(MEM_ID VARCHAR(10) NOT NULL PRIMARY KEY,SEQ INTEGER,NAME VARCHAR(20),DEPT_CD VARCHAR(5),TEAM_CD VARCHAR(5),GRADE_CD VARCHAR(2),MOBILE VARCHAR(20),HIREDATE VARCHAR(8),PHOTO_NAME VARCHAR(100),PHOTO_PATH VARCHAR(300),STATUS VARCHAR(1),PROJECT VARCHAR(100),NOTE VARCHAR(500),REG_USER VARCHAR(10),REG_DATE DATE)
CREATE MEMORY TABLE TBGTMS_IDSV(TABLE_NAME VARCHAR(100),NEXT_ID DECIMAL)
CREATE MEMORY TABLE TB_PROFILE_M(MEMBER_NUMBER VARCHAR(9) NOT NULL,MEMBER_NAME VARCHAR(20) NOT NULL,MEMBER_SEX VARCHAR(1) NOT NULL,MEMBER_BIRTHDAY VARCHAR(8),MEMBER_ADDRESS VARCHAR(80),ETC VARCHAR(40),REGIST_ID VARCHAR(9) NOT NULL,UPDATE_ID VARCHAR(9) NOT NULL,REGIST_DATE VARCHAR(14) NOT NULL,UPDATE_DATE VARCHAR(14) NOT NULL)
CREATE MEMORY TABLE TB_CAREER_H(MEMBER_NUMBER VARCHAR(9) NOT NULL,BUSINESS_NAME VARCHAR(40) NOT NULL,WORK_DATE_FROM VARCHAR(8),WORK_DATE_TO VARCHAR(8),WORK_MONTH_TERM INTEGER,REGIST_ID VARCHAR(9) NOT NULL,UPDATE_ID VARCHAR(9) NOT NULL,REGIST_DATE VARCHAR(14) NOT NULL,UPDATE_DATE VARCHAR(14) NOT NULL)
CREATE MEMORY TABLE TB_SYM_BOARD_M(SEQ_NO VARCHAR(5) NOT NULL,USER_ID VARCHAR(10) NOT NULL,TITLE VARCHAR(50),PARENT_NO VARCHAR(5) DEFAULT '0',DESCR VARCHAR(4000),REG_USR_ID VARCHAR(10),REG_YMD DATE,UPD_USR_ID VARCHAR(10),UPD_YMD DATE,CONSTRAINT PK_TBGTMS_BOARD PRIMARY KEY(SEQ_NO,USER_ID))
CREATE MEMORY TABLE T_BRD_M(SEQ_NO INTEGER NOT NULL,PARENT_NO INTEGER NOT NULL,USR_ID VARCHAR(10),TITLE VARCHAR(50),DESC VARCHAR(4000),DEPTH VARCHAR(1),FILE_NAME VARCHAR(100),FILE_PATH VARCHAR(300),REG_ID VARCHAR(10),REG_YMD DATE,UPD_ID VARCHAR(10),UPD_YMD DATE,CONSTRAINT T_BRD_M_PK PRIMARY KEY(SEQ_NO,PARENT_NO))
CREATE MEMORY TABLE T_IDGEN_M(TABLE_NAME VARCHAR(100),NEXT_ID DECIMAL)
CREATE MEMORY TABLE T_CODE_M(CODE_ID VARCHAR(5) NOT NULL PRIMARY KEY,CODE_NM VARCHAR(50),USE_YN VARCHAR(2),ETC VARCHAR(100),REG_USER VARCHAR(10),REG_DATE DATE,UPD_USER VARCHAR(10),UPD_DATE DATE)
CREATE MEMORY TABLE PERSON(NAME VARCHAR,AGE INTEGER)
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 20
SET SCHEMA PUBLIC
