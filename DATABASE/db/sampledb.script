INSERT INTO T_BRD_M VALUES(297,297,'pilot','\ud504\ub808\uc784\uc6cd\uc744 \uc774\uc6a9\ud55c \uba54\uc77c\uc804\uc1a1 \ubaa8\ub4c8\ubb38\uc758','\uc790\ub3d9 \uba54\uc77c \uc804\uc1a1\ud504\ub85c\uadf8\ub7a8\uc744 \uac1c\ubc1c\ud558\ub824 \ud569\ub2c8\ub2e4. \ud604\uc7ac EMS \uba54\uc77c\uad00\ub9ac\ub294 \uad00\ub9ac \ud504\ub85c\uadf8\ub7a8\uc73c\ub85c\u000d\u000a\uc804\uc1a1\uc5d0 \ub300\ud55c \ubd80\ubd84\uc774 \ud655\uc778\ub418\uc5b4 \uc788\uc9c0 \uc54a\uc2b5\ub2c8\ub2e4.\u000d\u000a\u000d\u000a\uc804\uc1a1\ubd80\ubd84\uc740 \ubcc4\ub3c4\ub85c \ucd94\uac00\ud574\uc57c \ud558\ub294\uc9c0\uc694? \uc544\ub2d8 back\ub2e8\uc5d0\uc11c \ucc98\ub9ac\ub418\uace0 \uc788\ub294\uac74\uac00\uc694?\u000d\u000a\uad6c\ud604\ud558\ub824\uba74 \uc5b4\ub5bb\uac8c \ud574\uc57c\ud558\ub294\uc9c0 \uae30\uc220\uc801\uc778 \ub2f5\ubcc0 \uae30\ub2e4\ub9bd\ub2c8\ub2e4  \u000d\u000a','1','','','pilot','2010-11-08','pilot','2010-11-08')
INSERT INTO T_BRD_M VALUES(298,297,'pilot','\ud504\ub808\uc784\uc6cd\uc744 \uc774\uc6a9\ud55c \uba54\uc77c\uc804\uc1a1 \ubaa8\ub4c8\ubb38\uc758','EMS \uba54\uc77c\uad00\ub9ac\ub294 \ub9d0\uc500\ud558\uc2e0\ub300\ub85c \uad00\ub9ac\ub97c \ubaa9\uc801\uc73c\ub85c \ud558\uace0 \uc788\uc2b5\ub2c8\ub2e4.\u000d\u000a\uc804\uc1a1\uc5d0 \uad00\ub828\ub41c \uad6c\ud604\ubd80\ubd84\uc740 \uc2e4\ud589\ud658\uacbd \uac00\uc774\ub4dc\uc5d0 Mail \ubd80\ubd84\uc744 \ucc38\uc870\ud558\uc2dc\uae38 \ubc14\ub78d\ub2c8\ub2e4.\u000d\u000a\uae30\ubcf8\uc801\uc73c\ub85c \ud655\uc7a5\ud55c \uac83\uc740 \uc544\ub2c8\uace0 \uae30\uc874 Mail API \ub97c \uc774\uc6a9\ud558\ub294 \uc608\ub97c \ubcf4\uc5ec\uc8fc\uace0 \uc788\uc2b5\ub2c8\ub2e4. \u000d\u000a','2','','','pilot','2010-11-08','pilot','2010-11-08')
INSERT INTO T_BRD_M VALUES(299,299,'pilot','Controller \uc758 \ud30c\ub77c\ubbf8\ud130\ub97c Map \uc73c\ub85c \ucc98\ub9ac\ubb38\uc81c \ubb38\uc758','demo \uc758 Controller \uc0d8\ud50c\uc744 \ubcf4\uba74 \uc544\ub798\uc640 \uac19\uc774 VO \ub97c \uc124\uc815\ud558\uc5ec \ub118\uaca8\uc8fc\uac8c \ub418\uc5b4\uc788\ub294\ub370 VO \ub300\uc2e0\uc5d0 Map \uc744 \ud1b5\ud558\uc5ec request \ud30c\ub77c\ubbf8\ud130\uac12\uc744 \ubc1b\uace0 \uc2f6\uc2b5\ub2c8\ub2e4.\u000d\u000a\u000d\u000apublic String selectSampleList(@ModelAttribute("searchVO") SampleDefaultVO searchVO, ModelMap model) throws Exception\u000d\u000a\u000d\u000a\uc804\uc5d0 \uad50\uc721\ubc1b\uc73c\uba74\uc11c \uac15\uc0ac\ub2d8\uaed8 \uc9c8\ubb38\uc744 \ub4dc\ub838\ub294\ub370 \ub41c\ub2e4\uace0 \ud558\uc2dc\ub358\ub370, \uc81c\uac00 \uc124\uc815\uc744 \uc798\ubabb\ud574\uc11c \uadf8\ub7f0\uc9c0 \uac12\uc774 \uc54a\ub118\uc5b4\uc635\ub2c8\ub2e4. \u000d\u000a\uc5b4\ub5a4\uc2dd\uc73c\ub85c \ubcc0\uacbd\uc744 \ud558\uba74 \ub420\uae4c\uc694? \uadf8\ub9ac\uace0 Map \uc744 \uc0ac\uc6a9\ud588\uc744 \uacbd\uc6b0 Vaildation \uccb4\ud06c\ub97c \uc5b4\ub5bb\uac8c \ud574\uc57c\ud558\ub098\uc694??  \u000d\u000a \u000d\u000a','1','','','pilot','2010-11-08','pilot','2010-11-08')
INSERT INTO T_BRD_M VALUES(300,299,'pilot','Controller \uc758 \ud30c\ub77c\ubbf8\ud130\ub97c Map \uc73c\ub85c \ucc98\ub9ac\ubb38\uc81c \ubb38\uc758','egovframework \uc5d0\uc11c \uc81c\uacf5\ub418\ub294 util \uc911\u000d\u000aCommandMapArgumentResolver \ub77c\ub294 \ud074\ub798\uc2a4\uac00 \uc788\ub294\ub370 request \ub97c \ud1b5\ud574 \ub118\uc5b4\uc624\ub294 parameter \ub370\uc774\ud0c0\ub97c (request.getParameterNames() \uc774\uc6a9) \uaebc\ub0b4\uc640 map \uc73c\ub85c \ub9ac\ud134\uc744 \ud574\uc8fc\uace0 \uc788\uc2b5\ub2c8\ub2e4. Controller \uc758 \uba54\uc18c\ub4dc \ud30c\ub77c\ubbf8\ud130\uba85\uc744 commandMap \uc73c\ub85c \uc4f0\uc2dc\uba74 \uc790\ub3d9 \ubc14\uc778\ub529\ub41c map \uc744 \ubc1b\uac8c \ub429\ub2c8\ub2e4. \u000d\u000apublic String getDepartmentList(Map commandMap, ModelMap model){\u000d\u000a\u000d\u000a\uc124\uc815\uc740 \uc544\ub798\uc640 \uac19\uc774 \ud558\uba74 \ub429\ub2c8\ub2e4.\u000d\u000a<bean class="org.springframework.web.servlet.mvc.annotation.AnnotationMethodHandlerAdapter">\u000d\u000a<property name="customArgumentResolvers">\u000d\u000a<list>\u000d\u000a<bean class="egovframework.rte.ptl.mvc.bind.CommandMapArgumentResolver" />\u000d\u000a</list>\u000d\u000a</property>\u000d\u000a</bean> ','2','','','pilot','2010-11-08','pilot','2010-11-08')
INSERT INTO T_BRD_M VALUES(301,301,'pilot','\ub77c\uc774\ube0c\ub7ec\ub9ac Build Path \ubd80\ubd84\uc5d0 \ube68\uac04\uc5d0\ub7ec \ubc1c\uc0dd \ubb38\uc81c','\ub2e4\uc6b4\ub85c\ub4dc \uba54\ub274 \uc911 \uac1c\ubc1c\ud658\uacbd>\uad6c\ud604\ub3c4\uad6c(ImplementationTool), >eclipse-egovframe-win32-1.0.0.zip \ub97c \ubc1b\uc544 \uc790\ub8cc\uc2e4 > \ub3d9\uc601\uc0c1 \uad50\uc7ac\ucc98\ub7fc \ud574\ubd24\uc2b5\ub2c8\ub2e4.\u000d\u000a\ub3d9\uc601\uc0c1\uc740 \uc2e4\ud589\ud658\uacbd\uac00\uc774\ud2b8 \ud504\ub85c\uadf8\ub7a8 \uc2e4\ud589\ubd80\ubd84\uc778\ub370\uc694...\u000d\u000alib\uac00 \uc548\ub9de\ub294\uc9c0 \uacc4\uc18d lib Build Path \ubd80\ubd84\uc5d0 \ube68\uac04\uc5d0\ub7ec \ubc1c\uc0dd\ud569\ub2c8\ub2e4.\u000d\u000a\u000d\u000a\uc774 \uc5d0\ub7ec\ub294 \uc5b4\ub5a0\ud55c \uc0c1\ud669\uc5d0\uc11c \ub098\uc624\ub294 \uac74\uac00\uc694? ','1','','','pilot','2010-11-08','pilot','2010-11-08')
INSERT INTO T_BRD_M VALUES(302,301,'pilot','\ub77c\uc774\ube0c\ub7ec\ub9ac Build Path \ubd80\ubd84\uc5d0 \ube68\uac04\uc5d0\ub7ec \ubc1c\uc0dd \ubb38\uc81c','\ud604\uc7ac \uc2e4\ud589\ud658\uacbd \ub3d9\uc601\uc0c1 \uac00\uc774\ub4dc\uc5d0\ub294 \uae30\ubcf8 maven \ub808\ud30c\uc9c0\ud1a0\ub9ac\uac00 \uad6c\uc131\uc774 \ub41c \uc0c1\ud0dc\uc5d0\uc11c \uad50\uc721\uc744 \uc9c4\ud589\ud558\uc600\uc2b5\ub2c8\ub2e4.\u000d\u000a\ub9cc\uc57d lib Build Path \ubd80\ubd84\uc5d0 \ube68\uac04 \uc5d0\ub7ec\uac00 \ubc1c\uc0dd\ud588\ub2e4\uba74 \uae30\ubcf8\uc801\uc778 library\uac00 \uc81c\ub300\ub85c \uc124\uc815\ub418\uc9c0 \uc54a\uc740 \uacbd\uc6b0\uc774\uba70 \uc774\ub97c \ud574\uacb0\ud558\uae30 \uc704\ud574\uc11c\ub294 \ud3ec\ud138\uc0ac\uc774\ud2b8(www.egovframe.go.kr)\uc758 \uba54\uc778 \uc624\ub978\ucabd \ubd80\ubd84\uc5d0 FAQ \uba54\uc778\uc73c\ub85c "Q \uacf5\ud1b5\ucef4\ud3ec\ub10c\ud2b8\uc5d0 \ud544\uc694\ud55c ...." \uc774\ub77c\ub294 \uae00\uc744 \ucc38\uc870\ud558\uc2dc\uc5b4 \ud45c\uc900\ud504\ub808\uc784\uc6cc\ud06c\uc0ac\uc5c5\ub2e8\uc5d0\uc11c \uc81c\uacf5\ud55c pom.xml\uc744 \uc0ac\uc6a9\ud558\uc2dc\uba74 \ud574\uacb0\ub420 \uac83 \uac19\uc2b5\ub2c8\ub2e4. ','2','','','pilot','2010-11-08','pilot','2010-11-08')
INSERT INTO T_BRD_M VALUES(303,303,'pilot','\ud504\ub85c\uc81d\ud2b8 \ud328\ud0a4\uc9c0 \uc0dd\uc131 \uad00\ub828 \ubb38\uc758','src.main.java.egovframework \ud328\ud0a4\uc9c0\uc640 \uac19\uc740 \ub808\ubca8\ub85c\u000d\u000asrc.main.java.portmis\ub77c\ub294 \ud328\ud0a4\uc9c0\ub97c \ub9cc\ub4e4\uc5b4\uc11c \ube44\uc9c0\ub2c8\uc2a4 \uc18c\uc2a4\ub97c \uad6c\ud604\ud560\ub824\uace0 \ud569\ub2c8\ub2e4.\u000d\u000a\uc774\ub54c \ubcc0\uacbd\ud558\uac70\ub098 \ucd94\uac00\ud574\uc918\uc57c \ub418\ub294 \uc124\uc815\uc774 \uc788\uc73c\uba74 \uc54c\ub824\uc8fc\uc138\uc694 ','1','','','pilot','2010-11-08','pilot','2010-11-08')
INSERT INTO T_BRD_M VALUES(304,303,'pilot','\ud504\ub85c\uc81d\ud2b8 \ud328\ud0a4\uc9c0 \uc0dd\uc131 \uad00\ub828 \ubb38\uc758','\ub9cc\ub4dc\uc2dc\uace0\uc790 \ud558\ub294 \ud328\ud0a4\uc9c0\ub294 \ubc18\ub4dc\uc2dc egovframework \uc758 \ud328\ud0a4\uc9c0 \uad6c\uc870\ub85c \ub9cc\ub4dc\uc2e4 \ud544\uc694\ub294 \uc5c6\uc2b5\ub2c8\ub2e4.\u000d\u000a\uc6d0\ud558\uc2dc\ub294 \ud615\ud0dc\ub85c \ud328\ud0a4\uc9c0\ub97c \ub9cc\ub4dc\uc2e0 \ub2e4\uc74c aop \uc124\uc815\uc0c1\uc758 pointcut \ud574\ub2f9\ud558\ub294 \ubd80\ubd84(\uc608\ub97c \ub4e4\uc790\uba74 expression="execution(* egovframework.lab.aop..Xml*Impl.*(..))") ) \uc5d0 \ud328\ud0a4\uc9c0\uc815\ubcf4\ub97c \uc218\uc815\ud574\uc8fc\uc5b4\uc57c \ud569\ub2c8\ub2e4. \ub610\ud55c component scan \uc2dc\uc758 base-package \uc5d0 \uc720\uc758\ud558\uc2dc\uba74 \ub429\ub2c8\ub2e4.\u000d\u000a(\uc608\ub97c \ub4e4\uc5b4 ) \u000d\u000a\uc5ec\uae30\uc11c\ub294 egovframework \uc774 \ucd5c\uc0c1\ub2e8\uc758 \ud328\ud0a4\uc9c0\uba85\uacfc \uc6d0\ud558\uc2dc\ub294 \ud328\ud0a4\uc9c0\uc758 \ucd5c\uc0c1\ub2e8(mypackage) \uc744 \uba85\uc2dc\ud574\uc8fc\uc2dc\uba74 \ub429\ub2c8\ub2e4. \u000d\u000a \u000d\u000a','2','','','pilot','2010-11-08','pilot','2010-11-08')
