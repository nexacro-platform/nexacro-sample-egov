INSERT INTO T_BRD_M VALUES(306,305,'pilot','controller\ub2e8\uc5d0\uc11c html \ud638\ucd9c\ubb38\uc81c','Controller \uc5d0\uc11c html \uc744 \ubd80\ub974\ub294 \ubc29\ubc95\uc740 \ub2e4\ub978 \ub2e4\ub978 url \uc758 \uba54\uc18c\ub4dc\ub97c \ubd80\ub974\ub294 \ubc29\uc2dd\uacfc \uc720\uc0ac\ud569\ub2c8\ub2e4.\u000d\u000a\uc608\ub97c \ub4e4\uc790\uba74 \uc544\ub798\uc640 \uac19\uc740 \ud615\ud0dc\ub85c \ub118\uae38\uc218 \uc788\uc2b5\ub2c8\ub2e4. \u000d\u000a@RequestMapping(value = "/hello.do")\u000d\u000apublic String redirect() {\u000d\u000areturn "redirect:/hello.html";\u000d\u000a}\u000d\u000a\ub2e8\uc9c0 \ucc28\uc774\ub294 redirect \uc640 forward \ub97c \ubd99\uc774\uace0 html \ud30c\uc77c\uc744 \uba85\uc2dc\ud574\uc11c \ub118\uae30\uba74 \ub429\ub2c8\ub2e4.\u000d\u000aredirect \uc640 forward \uc758 \ucc28\uc774\ub294 \ub124\uc774\ubc84\ub098 \ub2e4\uc74c\uc5d0\uc11c servlet redirect forward \ub85c \uac80\uc0c9\ud558\uc2dc\uba74 \ucc3e\uc544 \ubcf4\uc2e4\uc218 \uc788\uc2b5\ub2c8\ub2e4.  \u000d\u000a','2','','','pilot','2010-11-08','pilot','2010-11-08')
INSERT INTO T_BRD_M VALUES(307,307,'pilot','ibatis sqlMap, resultClass \uc7a5\ub2e8\uc810 \ubb38\uc758','ibatis sqlMap\uc758 SELECT\uad6c\ubb38\uc5d0\uc11c resultClass \uc5d0 `\uc77c\ubc18VO`, `hashMap`, `ListOrderedMap` \uc744 \uac01\uac01 \uc0ac\uc6a9\ud558\uc5ec \uac1c\ubc1c\uc2dc \uc7a5,\ub2e8\uc810 \ub610\ub294 \uc131\ub2a5 \uadf8\ub9ac\uace0, \uc774\ub7f4 \ub550 \uc774 \ud074\ub798\uc2a4\ub97c \uc0ac\uc6a9\ud55c\ub2e4 \ub4f1\uc758 \uad6c\uccb4\uc801\uc778 \ub0b4\uc6a9\uc744 \ubd80\ud0c1\ub4dc\ub9bd\ub2c8\ub2e4.  \u000d\u000a','1','','','pilot','2010-11-08','pilot','2010-11-08')
INSERT INTO T_BRD_M VALUES(308,307,'pilot','ibatis sqlMap, resultClass \uc7a5\ub2e8\uc810 \ubb38\uc758','"\uc0ac\uc6a9\uc790\uc758 \ud504\ub85c\uc81d\ud2b8\uc5d0 \ub9de\uac8c \uad6c\uc131\ud558\uc154\uc57c \ud569\ub2c8\ub2e4"\ub77c\uace0 \uc2dc\uc791\ud558\uc5ec\uc57c \ud560\uac83 \uac19\uc2b5\ub2c8\ub2e4.\u000d\u000a\uac04\ub2e8\ud558\uac8c \uc124\uba85\ub4dc\ub9ac\uba74 ibatis sqlMap\uc744 \uc0ac\uc6a9\uc2dc ibatis\uc5d0\uc11c\ub294 VO\ub97c \uad8c\uc7a5\ud55c\ub2e4\uace0 \ud569\ub2c8\ub2e4. \u000d\u000a\uc774\uc720\ub294 \uc124\uacc4\uc5d0 \ub300\ud55c \ud56d\ubaa9\uc744 \ucf54\ub529(getter /setter)\uc744 \ud1b5\ud558\uc5ec \uc0ac\uc6a9\ud558\ubbc0\ub85c \uc131\ub2a5\uc0c1\uc5d0 \uc6b0\uc218\ud568\uc744 \ub098\ud0c0\ub0b8\ub2e4\uace0 \ud569\ub2c8\ub2e4. \uadf8\ub7ec\ub098 \ucf54\ub529\uc591\uc774 \ub9ce\uc544\uc9c0\uace0 \uc790\ub3d9\uc73c\ub85c \ucc98\ub9ac\ud558\ub294 \ubd80\ubd84\uc774\ub098 \ub9ac\ud50c\ub809\uc158\ub4f1\uc5d0 \ub300\ud558\uc5ec \uc131\ub2a5\uc0c1\uc5d0 \ubb38\uc81c\uac00 \uc788\ub2e4\ub294 ibatis\ucabd\uc5d0 \ud3ec\ub7fc\uc5d0 \uae00\uc744 \ubcf8\uc801\uc774 \uc787\uc2b5\ub2c8\ub2e4.\u000d\u000a\u000d\u000ahashMap\uc744 \uc0ac\uc6a9\ud560\uacbd\uc6b0 \ub9ac\ud50c\ub809\uc158\ub4f1\uc5d0 \ub300\ud558\uc5ec VO\ubcf4\ub2e4\ub294 \uc88b\uc740 \uc131\ub2a5\uc744 \ub098\ud0c0\ub0b8\ub2e4\uace0 \ud569\ub2c8\ub2e4.\u000d\u000a\u000d\u000a\uac04\ub7b5\ud558\uac8c \uc124\uba85\ub4dc\ub838\uc9c0\ub9cc \ub450 \uae30\ub2a5\ub2e4 \uc0ac\uc6a9\uc2dc \uc7a5\ub2e8\uc810\uc774 \uc874\uc7ac\ub97c \ud569\ub2c8\ub2e4.\u000d\u000ahttp://www.ksug.org \uc5d0 \uac00\uc2dc\uba74 MAP\uc5d0 \ub300\ud55c \uacac\ud574\ub97c \ubd84\uc11d\ud558 \uc790\ub8cc\uac00 \uc788\uc73c\ub2c8 \ucc38\uace0\ud558\uc2dc\uba74 \ub9ce\uc740 \ub3c4\uc6c0\uc774 \ub420\uac83 \uac19\uc2b5\ub2c8\ub2e4  \u000d\u000a','2','','','pilot','2010-11-08','pilot','2010-11-08')
INSERT INTO T_BRD_M VALUES(309,309,'pilot','egovMap\uc758 \uc5ed\ud65c\uc5d0 \ub300\ud55c \ubb38\uc758','\ucffc\ub9ac \ub0a0\ub824\uc11c \uacb0\uacfc\uac12 \ub2f4\ub294 \ubd80\ubd84\uc5d0\uc11c resultClass="egovMap"\u000d\u000a\u000d\u000a\u000d\u000a\uc774\ub807\uac8c \ub9e4\uce6d\ub418\uc5b4 \ub2f4\uae30\ub294\uac83 \uac19\uc740\ub370 \uc5ec\uae30\uc11c EgovMap\uac00 \ud558\ub294 \uc5ed\ud560\uc774 \ubb54\uac00\uc694? \ub77c\uc774\ube0c\ub7ec\ub9ac\ub85c \uc81c\uacf5\uc774 \ub418\uc788\uc5b4\uc11c \uc751\uc6a9\ud574\uc11c \uc4f0\ub294 \ubc29\ubc95\uc744 \ubaa8\ub974\uaca0\ub124\uc694..\u000d\u000a\u000d\u000ajsp\ub2e8\uc5d0\uc11c \ubaa9\ub85d\uc73c\ub85c \ubfcc\ub9b4\ub54c\u000d\u000a\uc774\ub807\uac8c \uc0ac\uc6a9\ud558\ub358\ub370 \uc5ec\uae30\uc5d0\uc11c .inqireCo \uc774\uac83\uc740 \ubb58 \uc758\ubbf8\ud558\ub294\uac74\uac00\uc694? \u000d\u000aVO\uc5d0 \ub2f4\uc544\uc11c \uc628\uac74 \uc544\ub2cc\uac70 \uac19\uace0 \uc704\uc5d0\uc11c \ub9d0\ud55c\ubc14\uc640 \uac19\uc774 EgovMap\uc5d0 \ub2f4\uc544\uc11c \uc628\uac70 \uac19\uc740\ub370.. \uc5b4\ub5a4\uc2dd\uc73c\ub85c \ub9e4\ud551\ub418\ub294\uc9c0 \uc880 \uc54c\ub824\uc8fc\uc138\uc694.  \u000d\u000a','1','','','pilot','2010-11-08','pilot','2010-11-08')
INSERT INTO T_BRD_M VALUES(310,309,'pilot','egovMap\uc758 \uc5ed\ud65c\uc5d0 \ub300\ud55c \ubb38\uc758','EgovMap\uc740 Map\ud615\ud0dc\uc758 \ud074\ub798\uc2a4\ub85c iBatis parameterClass \ub610\ub294 resultClass\uc5d0\uc11c \uc0ac\uc6a9\ud560 \uc218 \uc788\ub3c4\ub85d \uc81c\uacf5\ub418\ub294 \ud074\ub798\uc2a4\uc785\ub2c8\ub2e4. (\uc77c\ubc18\uc801\uc778 HashMap \ud615\ud0dc\uc758 \ud074\ub798\uc2a4\ub97c \uc0ac\uc6a9 \ubc29\ubc95\uacfc \ub3d9\uc77c)\u000d\u000a\ub2e4\ub9cc, \ucd94\uac00\uc801\uc73c\ub85c Key\uc5d0 \ud574\ub2f9\ub418\ub294 \ubd80\ubd84\uc744 CamelCase \ubc29\uc2dd\uc73c\ub85c \uc801\uc6a9\ud558\ub294 \ubd80\ubd84\uc774 \ucd94\uac00 \uc801\uc6a9\ub418\uc5b4 \uc788\uc2b5\ub2c8\ub2e4.\u000d\u000a\u000d\u000aJSP \ubd80\ubd84\uc758 inqireCo \ubd80\ubd84\uc740 Controller \ubd80\ubd84\uc5d0\uc11c model\ub85c \ub118\uaca8\uc9c4 resultList \uac1d\uccb4(\ubcf4\ud1b5 List\uac1d\uccb4)\ub97c forEach\ub97c \ud1b5\ud574 \uac1c\ubcc4 item(List\uac1d\uccb4\uc758 get \uba54\uc18c\ub4dc\ub97c \ud1b5\ud574 \uc5bb\uc74c) \uac1d\uccb4\uc758 getInqireCo() \uba54\uc18c\ub4dc\ub97c \ud1b5\ud574 \uc5bb\uc5b4\uc9c4 \uac12\uc785\ub2c8\ub2e4.\u000d\u000a\u000d\u000a\uc774 \ubd80\ubd84\uc740 JSTL(JSP Standard Tag Library) \ub610\ub294 EL(Express Lanaguage)\ub97c \ucc38\uc870\ud558\uc2dc\uba74 \ub420 \uac83 \uac19\uc2b5\ub2c8\ub2e4. ','2','','','pilot','2010-11-08','pilot','2010-11-08')
INSERT INTO T_BRD_M VALUES(311,311,'pilot','message properites \ud55c\uae00\uc9c0\uc6d0 \uad00\ub828 \ubb38\uc758','\uba54\uc138\uc9c0 \uad00\ub828 \ud574\ub2f9 \ud30c\uc77c\uc744 \ubcf4\uc558\ub354\ub2c8 \uba54\uc2dc\uc9c0\uba85\uc774 \uc774\uc0c1\ud558\uac8c \ub098\uc654\ub124\uc694.\u000d\u000a\ud655\uc778\ub610\ub294 \ubcc0\uacbd\ud558\ub824\uba74 \uc5b4\ub5a1\uac8c\ud574\uc57c\ud558\ub098\uc694?\u000d\u000a\uc6d0\uc778\uc740 \ubb34\uc5c7\uc778\uac00\uc694? ','1','','','pilot','2010-11-08','pilot','2010-11-08')
INSERT INTO T_BRD_M VALUES(312,311,'pilot','message properites \ud55c\uae00\uc9c0\uc6d0 \uad00\ub828 \ubb38\uc758','\ud55c\uae00\uc5d0 \ub300\ud55c properties \ud30c\uc77c\uc758 \uacbd\uc6b0 \uc2e4\uc81c \uc0ac\uc6a9\ud558\uae30 \uc804\uc5d0 JDK\uc5d0 \ubc30\ud3ec\ub418\ub294 native2ascii \ud504\ub85c\uadf8\ub7a8\uc73c\ub85c \uc720\ub2c8\ucf54\ub4dc \ud615\uc2dd\uc73c\ub85c \ubcc0\ud658\uc744 \ud574\uc8fc\uc5b4\uc57c \uc0ac\uc6a9\uac00\ub2a5\ud569\ub2c8\ub2e4.\u000d\u000a\ubcf4\uc2dc\ub294 \uc18d\uc131 \ud30c\uc77c\ub3c4 \ud55c\uae00\ub85c \uc791\uc131\ub41c \uac83\uc774 \uc720\ub2c8\ucf54\ub4dc \ud615\uc2dd\uc73c\ub85c \ubcc0\ud658\ub41c \uac83\uc785\ub2c8\ub2e4.\u000d\u000a\u000d\u000a\uc774\ub7f0 \uacbd\uc6b0 \ud55c\uae00\uc744 \uc54c\uc544\ubcf4\uae30\ub3c4 \uc5b4\ub835\uace0 \ub9e4\ubc88 \ubcc0\ud658\ud574\uc8fc\uc5b4\uc57c \ud558\ub294 \ubb38\uc81c\uac00 \ubc1c\uc0dd\ud558\uae30 \ub54c\ubb38\uc5d0\u000d\u000a\ubcf4\ud1b5 Eclipse \ud50c\ub7ec\uadf8\uc778\uc744 \ud65c\uc6a9\ud569\ub2c8\ub2e4.\u000d\u000a\uc778\ud130\ub137 \ub4f1\uc5d0\uc11c \uac80\uc0c9\ud574 \ubcf4\uc2dc\uba74 \uba87 \uac00\uc9c0 \ud50c\ub7ec\uadf8\uc778\uc774 \uc0ac\uc6a9\uac00\ub2a5\ud55c\ub370..\u000d\u000aProperties Editor\ub97c \ud65c\uc6a9\ud558\uc2dc\uba74 \ub420 \uac83 \uac19\uc2b5\ub2c8\ub2e4. \u000d\u000a\uad00\ub828 \ub0b4\uc6a9\uc740 \ub2e4\uc74c URL\uc744 \ucc38\uc870\ud558\uc2dc\uad6c\uc694..\u000d\u000ahttp://propedit.sourceforge.jp/index_en.html ','2','','','pilot','2010-11-08','pilot','2010-11-08')
INSERT INTO T_BRD_M VALUES(313,313,'pilot','\uacf5\ud1b5\ucef4\ud3ec\ub10c\ud2b8\uc6a9 pom.xml\uc744 \uc694\uccad\ud569\ub2c8\ub2e4.','\uacf5\ud1b5\ucef4\ud3ec\ub10c\ud2b8 \uc18c\uc2a4\ub4e4\uc744 import\ud558\uba74 \uad00\ub828\ub41c library(jar)\uac00 \uc5c6\uc5b4\uc11c maven \ube4c\ub4dc\uac00 \ub418\uc9c0 \uc54a\uc2b5\ub2c8\ub2e4.\u000d\u000a\uacf5\ud1b5\ucef4\ud3ec\ub10c\ud2b8\uc6a9 pom.xml\uc544\ub2c8 dependency \uc815\ubcf4\ub97c \ubd80\ud0c1\ub4dc\ub9bd\ub2c8\ub2e4. ','1','','','pilot','2010-11-08','pilot','2010-11-08')
INSERT INTO T_BRD_M VALUES(314,313,'pilot','\uacf5\ud1b5\ucef4\ud3ec\ub10c\ud2b8\uc6a9 pom.xml\uc744 \uc694\uccad\ud569\ub2c8\ub2e4.','\ucca8\ubd80\ub85c pom(\ubc30\ud3ec\uc6a9).xml\ub97c \uc62c\ub838\uc2b5\ub2c8\ub2e4.\u000d\u000a\uc774 \ud30c\uc77c\uc744 \ucc38\uc870\ud558\uc2dc\uba74 \ub420 \uac83 \uac19\uc2b5\ub2c8\ub2e4.\u000d\u000a\u000d\u000a\ucc38\uc870\ud558\uc2e4 \ub54c\uc5d0 \uae30\uc874 pom.xml\uc744 \uadf8\ub300\ub85c \ub36e\uc5b4\uc4f0\uc9c0 \ub9c8\uc2dc\uace0 pom(\ubc30\ud3ec\uc6a9).xml\uc5d0\uc11c..\u000d\u000a\ub2e4\uc74c\uc758 3\ubd80\ubd84\uc744 \uae30\uc874 pom.xml\uc5d0 \ucd94\uac00\ud574 \uc8fc\uc154\uc57c \uae30\uc874 pom.xml \uc124\uc815\uc744 \uc720\uc9c0\ud558\uc2e4 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\u000d\u000a- <properties> \ubd80\ubd84\u000d\u000a- <repositories> \ubd80\ubd84\uc758 <repository> \ud56d\ubaa9\ub4e4\u000d\u000a- <dependencies> \ubd80\ubd84\uc758 <dependency> \ud56d\ubaa9\ub4e4\u000d\u000a\u000d\u000a\uac10\uc0ac\ud569\ub2c8\ub2e4.  \u000d\u000a','2','pom(\ubc30\ud3ec\uc6a9).xml','C:\GTMS\workspace\gtms\pilot\files\board\pom(\ubc30\ud3ec\uc6a9)_1289183547875.xml','pilot','2010-11-08','pilot','2010-11-08')
INSERT INTO T_BRD_M VALUES(315,315,'pilot','\uacf5\ud1b5\ucef4\ud3ec\ub10c\ud2b8\uc5d0\uc11c \uc0ac\uc6a9\ud558\ub294 context-idgen.xml \uc62c\ub824\uc8fc\uc138\uc694.','ID Generation\ub97c \uc0ac\uc6a9\ud558\ub294 \uacbd\uc6b0 context-idgen.xml\uc5d0 \uc9c0\uc815\ud558\ub294\ub370..\u000d\u000a\u000d\u000a\uacf5\ud1b5 \ucef4\ud3ec\ub10c\ud2b8\uc5d0 \ub300\ud55c \uc804\uccb4 \ub0b4\uc6a9\uc744 \ucc38\uc870\ud558\ub824\uace0 \ud569\ub2c8\ub2e4.\u000d\u000a\u000d\u000a\uc804\uccb4 context-idgen.xml \uc804\uccb4\ub97c \uc62c\ub824\uc8fc\uc138\uc694.  \u000d\u000a','1','','','pilot','2010-11-08','pilot','2010-11-08')
